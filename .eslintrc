{
  "extends": ["next", "prettier", "eslint:recommended"],
  "plugins": ["unused-imports"],
  "rules": {
    "no-unused-vars": 1,
    "unused-imports/no-unused-imports": "error",
    "unused-imports/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "after-used",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
